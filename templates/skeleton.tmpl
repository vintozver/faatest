<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:og="http://ogp.me/ns#" xml:lang="en" lang="en">
	<head>
		{% block meta -%}
		<!-- base meta -->
		<meta http-equiv="content-type" content="text/html; charset=utf-8" />
		<link rel="shortcut icon" href="/favicon.ico" />
		<!-- jquery -->
		<script type="text/javascript"{{ {'src': {'netloc': '', 'path': '/lib/jquery/jquery.js'}|url}|xmlattr }}></script>
		<!-- application -->
		<link rel="stylesheet"{{ {'href': {'netloc': '', 'path': '/main.css'}|url}|xmlattr }} />
		<script type="text/javascript"{{ {'src': {'netloc': '', 'path': '/main.js'}|url}|xmlattr }}></script>
		{%- endblock %}
		<title>{% block title %}{# page title #}{% endblock %}</title>
		{% block styles -%}
		<!-- base styles -->
		{%- endblock %}
		{% block scripts -%}
		<!-- base scripts -->
		{%- endblock %}
	</head>
	<body>
		{% block whole_canvas -%}
		{% block header_content -%}
		<!-- base header -->
		{%- endblock %}
		{% block body_content -%}
		<!-- base body -->
		{%- endblock %}
		{% block footer_content -%}
		<!-- base footer  -->
		{%- endblock %}
		{%- endblock whole_canvas %}
		<div class="footer_links">Developed by Vitaly Greck for UAB Paraloftas and ChutingStar Enterprises Inc.</div>
	 </body>
</html>
